                      "type": "object",
                      "properties": {
                        "type": {
                          "title": "Credentials Type",
                          "description": "Google Cloud Platform account type.",
                          "type": "string",
                          "default": "gcp_credential_path",
                          "const": "gcp_credential_path"
                        },
                        "path": {
                          "title": "Path",
                          "description": "Path of the file containing the GCP credentials info",
