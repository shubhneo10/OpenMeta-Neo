                        "projectId": {
                          "title": "GCP Project ID",
                          "description": "GCP Project ID to parse metadata from",
                          "oneOf": [
                            {
                              "title": "Single Project ID",
                              "type": "string"
                            },
                            {
                              "title": "Multiple Project ID",
                              "type": "array",
